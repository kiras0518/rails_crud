<%= search_form_for @q do |f| %>
    <div class="col-md-4">
        <div class="input-group">
            <%= f.search_field :title_cont ,class: 'form-control'%>
              <span class="input-group-btn">
                <%= f.submit class: 'btn btn-default' %>
              </span>
        
        </div>
    </div>
    
    <%= f.select :status_in, Task.statuses.to_a.map { |w| [w[0].humanize, w[1]] },{include_blank: true}, {class: 'btn btn-default btn-sm dropdown-toggle'} %> 
   
<% end %>
